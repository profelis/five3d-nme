package net.badimon.five3D;

/**
 * The Version class contains data about the current FIVe3D package version.
 */
class Version {

	/**
	 * Indicates the language type of the FIVe3D package.
	 */
	public static inline var type:String = "haxe - Flash";
	/**
	 * Indicates the version number of the FIVe3D package.
	 */
	public static inline var version:String = "1.0.0";
	/**
	 * Indicates the release date of the FIVe3D package current version.
	 */
	public static inline var date:String = "2011-08-02";
}